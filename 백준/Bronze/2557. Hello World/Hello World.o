section .data
    msg db 'Hello World!', 10, 0
    msg_len equ $ - msg - 1

section .text
    global main

main:
    ; write system call
    mov rax, 1          ; sys_write
    mov rdi, 1          ; stdout
    mov rsi, msg        ; message
    mov rdx, msg_len    ; message length
    syscall
    
    xor rax, rax        ; Set return value to 0
    ret